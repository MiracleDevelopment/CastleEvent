<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@android:color/white">
    <item>
        <selector>
            <item
                android:drawable="@drawable/custom_background_order_tickets"
                android:state_pressed="false">

            </item>
        </selector>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <stroke
                android:width="2dp"
                android:color="@color/colorStrokeTickets"
                android:dashWidth="2dp" />
            <padding
                android:bottom="2dp"
                android:left="15dp"
                android:right="15dp"
                android:top="2dp" />
        </shape>
    </item>
</ripple>